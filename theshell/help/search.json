---
layout: none
---

[
    {% for page in site.pages %}
        {% if page.layout == 'helpPage' %}
            {
                "title": "{{ page.title }}",
                "contents": {{ page.content | jsonify | strip_html }},
                "url": "{{ site.baseurl }}{{ page.url }}"
            }
        {% unless forloop.last %},{% endunless %}
        {% endif %}
    {% endfor %}
    {}
]
